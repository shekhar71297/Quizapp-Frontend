{
  "name": "quizzapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@material-ui/core": "^4.12.4",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.3",
    "@mui/styled-engine-sc": "^5.12.0",
    "@reduxjs/toolkit": "^2.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.1",
    "cdbreact": "^1.5.18",
    "chart.js": "^4.4.3",
    "concurrently": "^8.2.0",
    "font-awesome": "^4.7.0",
    "init": "^0.1.2",
    "jest": "^29.7.0",
    "jest-sonar": "^0.2.16",
    "jest-sonar-reporter": "^2.0.0",
    "moment": "^2.30.1",
    "msw": "^2.2.1",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-redux": "^8.1.3",
    "react-scripts": "5.0.1",
    "recharts": "^2.12.7",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.2",
    "styled-components": "^5.3.11",
    "web-vitals": "^2.1.4",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "json-server": "cd src/database && json-server --watch -p 8888 quizapp.json",
    "start:dev": "concurrently \" npm start\" \"npm run json-server\" ",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false --silent",
    "test-coverage": "npm test -- --env=jsdom --coverage --testResultsProcessor ./node_modules/jest-sonar-reporter/index.js || true",
    "test:watch": "react-scripts test --watch",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jestSonar": {
    "reportPath": "./coverage",
    "reportFile": "sonar-report.xml",
    "indent": 4
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/index.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@babel/runtime|axios)/)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-router-dom": "^6.14.2"
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "index.js",
  "author": "",
  "license": "ISC"
}
